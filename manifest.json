  "shortcuts": [
    {
      "name": "AI Strength Test",
      "short_name": "AI Test",
      "description": "Start an AI-powered strength test",
      "url": "/?action=ai-test",
      "icons": [
        {
          "src": "assets/icons/shortcut-ai-test.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Log Training",
      "short_name": "Log Workout",
      "description": "Log a new training session",
      "url": "/?action=log-training",
      "icons": [
        {
          "src": "assets/icons/shortcut-log-training.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "View Dashboard",
      "short_name": "Dashboard",
      "description": "View your performance dashboard",
      "url": "/?action=dashboard",
      "icons": [
        {
          "src": "assets/icons/shortcut-dashboard.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://athleteai.app/manifest.json"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "capture_links": "new-client",
  "protocol_handlers": [
    {
      "protocol": "web+athleteai",
      "url": "/?protocol=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/?action=import-data",
      "accept": {
        "application/json": [".json"],
        "text/csv": [".csv"]
      }
    }
  ],
  "share_target": {
    "action": "/?action=share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*", "video/*", "application/json"]
        }
      ]
    }
  },
  "permissions": [
    "camera",
    "microphone",
    "geolocation",
    "notifications",
    "persistent-storage"
  ],
  "features": [
    "Cross Platform",
    "fast",
    "simple"
  ],
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7"
